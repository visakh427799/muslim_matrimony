                    <script>
                        function fi() {

                            var d = new Date();
                            var CurrentYr = d.getFullYear();
                            let dob = document.getElementById("db").value;
                            let Yr = dob.slice(0, 4);
                            let age = CurrentYr - Number(Yr);
                            console.log(age);

                            if (age < 18) {
                                Swal.fire(
                                    {
                                        position: 'top-end',
                                        title:'Age should be  greaterthan 18'
                                    }
                              )

                                document.getElementById("db").value = "";

                            }


                        }
                    </script>
