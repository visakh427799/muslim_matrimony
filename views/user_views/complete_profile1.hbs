    <div class="call-to-action  ">
        <div class="container">
            <div class=" container ">
                <div class="fp-margin-botm ">
                    <h2>Complete your Profile</h2>
                    <p class="text-small text-white">Tell us about your basic details; to finding your love</p>

                </div>

                <form action="/complete_profile1" method="POST">
                    <div class="form-row   text-white">
                        <div class="col-md-6  ">
                            <label for="">Marital Status</label>
                            <div class="control-group">
                                <select name="marital_status" class="custom-select" required>
                                    <option value="">Select</option>
                                    <option value="Unmarried" >Unmarried</option>
                                    <option value="Nikah Divorce">Nikah Divorce </option>
                                    <option value="Widowed/Widower">Widowed/Widower</option>
                                    <option value="Divorced">Divorced</option>
                                    <option value="Awaiting divorce">Awaiting divorce</option>

                                </select>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-6   ">
                                    <label for="">Children</label>
                                    <div class="control-group">
                                        <select name="children"  class="custom-select" required>
                                      
                                            <option value="">Select</option>
                                            <option value="Yes">Yes</option>
                                            <option value="No">No</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6   ">
                                    <label for="">Number of Children</label>
                                    <div class="control-group">
                                        <select name="no_children"  class="custom-select" required>
                                            <option value="">Select</option>
                                            <option value="nil" >nil</option>
                                            <option value="1" >1</option>
                                            <option value="2" >2</option>
                                            <option value="3" >3</option>
                                            <option value="Above 3" >Above 3</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>

                        
                        <div class="col-md-6  ">
                            <label for="">Date Of Birth</label>
                            <input id="db" type="date" value="" name="dob" class="form-control" placeholder="" required />
                        </div>

                        
                        <div class="col-md-6   ">
                            <label for="">Religion</label>
                            <div class="control-group">
                                <select class="custom-select"  onfocus="fi();" required name="religion">
                                    <option value="">Select</option>
                                    <option value="Islam">Islam</option>

                                </select>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="row">
                                <div class="col-md-6   ">
                                    <label for="">Height</label>
                                    <!--   -->
                                    <div class="control-group">
                                        <select class="custom-select" name="height"  required>
                                            <option value="">Select</option>
                                           {{#each heights}}
                                               <option value="{{this.height}}">{{this.height}}</option>
                                           {{/each}}
                                        </select>
                                    </div>
                                   
                                </div>
                                <div class="col-md-6   ">
                                    <label for="">Weight</label>
                                    <!-- 40kg to 120kg -->
                                    <div class="control-group">
                                        <select class="custom-select" name="weight" required>
                                            <option value="">Select</option>
                                             {{#each weights}}
                                               <option value="{{this.weight}}">{{this.weight}}</option>
                                           {{/each}}
                                           
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        








                        <div class="col-md-6   ">
                            <label for="">Sect</label>
                            <div class="control-group">
                                <select class="custom-select" required name="sect">
                                    <option>Select</option>
                                    <option value="Sunni">Sunni</option>
                                    <option value="Sunni EK">Sunni EK</option>
                                    <option value="Sunni AP">Sunni AP</option>
                                    <option value="Shafi Sunni">Shafi Sunni</option>
                                    <option value="Hanafi Sunni">Hanafi Sunni</option>
                                    <option value="Hanafi">Hanafi</option>
                                    <option value="Shafi">Shafi</option>
                                    <option value="Salafi">Salafi</option>
                                    <option value="Jamaat e Islami">Jamaat e Islami</option>
                                    <option value="Tablighi Jamaat">Tablighi Jamaat</option>
                                    <option value="Muslim">Muslim</option>
                                    <option value="Other">Other</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6   ">
                            <label for="">Complexion</label>
                            <div class="control-group">
                                <select class="custom-select" name="complexion" required>
                                    <option value="">select</option>
                                    <option value="Very Fair">Very Fair</option>
                                    <option value="Fair">Fair</option>
                                    <option value="Wheatish">Wheatish</option>
                                    <option value="Medium Brown">Medium Brown</option>
                                    <option value="Dark Brown">Dark Brown</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6   ">
                            <label for="">Body type</label>
                            <div class="control-group">
                                <select class="custom-select" name="body_type" required>
                                    <option value="">Select</option>
                                    <option value="Slim">Slim</option>
                                    <option value="Average">Average</option>
                                    <option value="Above Average">Above Average</option>
                                    <option value="Heavy">Heavy</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6   ">
                            <label for="">Physical Status</label>
                            <div class="control-group">
                                <select class="custom-select" required name="physical_status">
                                    <option value="">Select</option>
                                    <option value="Normal Person">Normal Person</option>
                                    <option value="Physically Challenged Person">Physically Challenged Person</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6   ">
                            <label for="">Mother Tongue</label>
                            <div class="control-group">
                                <select class="custom-select" required name="mother_tongue">
                                    <option>Select</option>
                                    <option value="Malayalam">Malayalam</option>
                                    <option value="English">English</option>
                                    <option value="Hindi">Hindi</option>
                                    <option value="Arabic">Arabic</option>
                                    <option value="Kannada">Kannada</option>
                                    <option value="Sindhi">Sindhi</option>
                                    <option value="Tamil">Tamil</option>
                                    <option value="Telugu">Telugu</option>
                                    <option value="Tulu">Tulu</option>
                                    <option value="Urdu">Urdu</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6   ">
                            <label for="">Education</label>
                            <div class="control-group">
                                <select class="custom-select" required name="education">
                                    <option>Select</option>
                                    <option value="Doctorate">Doctorate</option>
                                    <option value="Masters">Masters</option>
                                    <option value="Bachelors">Bachelors</option>
                                    <option value="Diploma">Diploma</option>
                                    <option value="Religious Education">Religious Education</option>
                                    <option value="Plus Two / Pre Degree">Plus Two / Pre Degree</option>
                                    <option value="SSLC">SSLC</option>
                                    <option value="Below SSLC">Below SSLC</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-6   ">
                            <label for="">Job</label>
                            <input type="text" id="ab" value="" name="job" class="form-control" placeholder="" required />
                        </div>
                    </div>
                     <script>
                                        function fi(){

                                           var d = new Date();
                                           var CurrentYr = d.getFullYear();
                                           let dob=document.getElementById("db").value;
                                           let Yr=dob.slice(0,4);
                                           let age=CurrentYr-Number(Yr);
                                           console.log(age);

                                           if(age<18){
                                                  Swal.fire(
                                                    'Age should be  greaterthan 18')
                                       
                                               document.getElementById("db").value="";
                                               
                                           }
                                          
                                           
                                        } 
                                    </script>
                    <div class=" d-flex justify-content-center mt-4">

                        <button type="submit" class="btn gnrl-btn  ">
                            Save & Continue
                        </button>

                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

